PGraphics pg;

void setup()
{
 size(1000,1000);
 pg = createGraphics(1000,1000);

}

void draw()
{
  pg.beginDraw();
  background(255);
  
  textSize(32);
  textAlign(CENTER);
  text("Get random battery", 400, 50, 200, 100); 
fill(0, 255, 0);


  pg.endDraw();
  image(pg, 0, 0);
  save("try.png");
}
